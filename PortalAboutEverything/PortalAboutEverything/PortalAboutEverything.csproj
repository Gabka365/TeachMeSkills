<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\PortalAboutEverything.Data\PortalAboutEverything.Data.csproj" />
    <ProjectReference Include="..\PortalAboutEverything.VideoServices\PortalAboutEverything.VideoServices.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="LocalizationResources\Game_Index.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Game_Index.resx</DependentUpon>
    </Compile>
    <Compile Update="LocalizationResources\Traveling_Index.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Traveling_Index.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="LocalizationResources\Game_Index.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Game_Index.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="LocalizationResources\Traveling_Index.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Traveling_Index.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
