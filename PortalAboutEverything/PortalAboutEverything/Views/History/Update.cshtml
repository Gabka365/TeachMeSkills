@model PortalAboutEverything.Models.History.HistoryUpdateViewModel


<form method="post"> 
    @Html.HiddenFor(x => x.Id)   
   <div>
       @Html.TextBoxFor( x=> x.Name, new {placeholder = "Название События"})
   </div>
    <div>
        @Html.TextBoxFor(x => x.YearOfEvent, new { placeholder = "Дата", @type = "number", })
    </div>
    <div>
        @Html.TextAreaFor(x => x.Description, new{placeholder = "Описание"})
    </div>
   <div>
       <input type="submit" href="/History/Index" value="Обновить" />
   </div>
</form>
 
        
    